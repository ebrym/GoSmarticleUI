{
  "expo": {
    "name": "Kotigo",
    "description": "Knowledge on the go.",
    "slug": "Kotigo",
    "privacy": "unlisted",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/kotigologo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.bridge57.kotigo"
    },
    "android": {
      "package": "com.bridge57.kotigo"
    }
 
  }
}
